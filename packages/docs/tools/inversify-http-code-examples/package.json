  "devDependencies": {
    "@hono/node-server": "1.18.2",
    "@inversifyjs/code-examples-devkit": "workspace:*",
    "@inversifyjs/http-core": "workspace:*",
    "@inversifyjs/http-express": "workspace:*",
    "@inversifyjs/http-express-v4": "workspace:*",
    "@inversifyjs/http-fastify": "workspace:*",
    "@inversifyjs/http-hono": "workspace:*",
    "@types/cookie-parser": "1.4.9",
    "@types/express": "5.0.3",
    "@types/express4": "npm:@types/express@4.17.23",
    "@types/node": "22.17.1",
    "@vitest/coverage-v8": "3.2.4",
    "cookie-parser": "1.4.7",
    "eslint": "9.33.0",
    "express": "5.1.0",
    "express4": "npm:express@4.21.2",
    "fastify": "5.5.0",
    "glob": "11.0.3",
    "hono": "4.9.1",
    "inversify": "7.7.1",
    "prettier": "3.6.2",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "tslib": "2.8.1",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "homepage": "https://inversify.io",
  "devEngines": {
    "node": "^22.10.2",
    "pnpm": "^10.13.1"
  },
  "license": "MIT",
  "name": "@inversifyjs/http-code-examples",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inversify/monorepo.git"
  },
  "scripts": {
    "build": "pnpm run build:cjs && pnpm run generate:examples",
    "build:cjs": "tsc --build tsconfig.cjs.json && pnpm exec foundation-ts-package-cjs ./lib/cjs",
    "build:clean": "rimraf generated lib",
    "format": "prettier --write ./src",
    "generate:examples": "node ./lib/cjs/scripts/generateExamples/index.mjs",
    "lint": "eslint ./src",
    "prebuild": "pnpm run build:clean",
    "test": "vitest run",
    "test:integration": "pnpm run test --project Integration",
    "test:coverage": "pnpm run test:unit --coverage",
    "test:uncommitted": "pnpm run test --changed=HEAD",
    "test:unit": "pnpm run test --project Unit"
  }
}
