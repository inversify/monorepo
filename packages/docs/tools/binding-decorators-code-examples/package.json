  },
  "devDependencies": {
    "@inversifyjs/code-examples-devkit": "workspace:*",
    "@types/node": "22.14.0",
    "@vitest/coverage-v8": "3.1.1",
    "eslint": "9.23.0",
    "glob": "11.0.1",
    "prettier": "3.5.3",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "tslib": "2.8.1",
